{
  "name": "use-router",
  "description": "router for use-* middlewares",
  "version": "0.4.2",
  "scripts": {
    "lint": "eslint src test",
    "clean": "/bin/rm -rf dist",
    "build": "npm run lint && npm run clean && babel -D -d dist src",
    "watch": "babel -w -D -d dist src",
    "test:only": "mocha --timeout 10000 --recursive --bail -r test/require.js --reporter spec test/**/*.test.js",
    "test": "npm run build && npm run test:only",
    "cover:compiled": "istanbul cover _mocha -- -r test/require.js --reporter spec test/**/*.test.js",
    "cover": "npm run build -- --source-maps both && npm run cover:compiled",
    "prepublish": "npm test"
  },
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">= 4"
  },
  "dependencies": {
    "path-to-regexp": "^1.2.1",
    "use-compose": "^0.4.0",
    "use-express-middlewares": "^0.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-full-node4": "^1.0.0",
    "babel-register": "^6.18.0",
    "eslint-config-easy": "^0.6.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.2.0",
    "supertest": "^2.0.1"
  },
  "homepage": "https://github.com/kiliwalk/use-router",
  "bugs": "https://github.com/kiliwalk/use-router/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiliwalk/use-router"
  },
  "author": {
    "name": "kiliwalk",
    "email": "kiliwalk@qq.com"
  },
  "license": "MIT",
  "keywords": [
    "route",
    "router",
    "middleware",
    "use",
    "all",
    "get",
    "post",
    "express",
    "promise",
    "rewrite"
  ]
}
